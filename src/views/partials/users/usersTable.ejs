<div class="list" id="tableManageUsers">
    <table>
        <thead>
            <tr>
                <th>user</th>
                <th>name</th>
                <th>mail</th>
                <th>role</th>
            </tr>
        </thead>

        <tbody>
            <%users.map( ({ user, name, mail, rol}) => {%>
                <tr>
                    <td><%= user %></td>
                    <td><%= name %></td>
                    <td><%= mail ? mail : 'N/A' %></td>
                    <td><%= rol %></td>
                </tr>
            <%})%>
        </tbody>
    </table>
</div>